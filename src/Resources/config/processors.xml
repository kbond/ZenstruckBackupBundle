<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="zenstruck_backup.processor.abstract_zip" class="Zenstruck\Backup\Processor\ZipArchiveProcessor" abstract="true" public="false">
            <argument /> <!-- name -->
            <argument type="collection" /> <!-- options -->
        </service>
        <service id="zenstruck_backup.processor.abstract_gzip" class="Zenstruck\Backup\Processor\GzipArchiveProcessor" abstract="true" public="false">
            <argument /> <!-- name -->
            <argument type="collection" /> <!-- options -->
        </service>
    </services>
</container>
